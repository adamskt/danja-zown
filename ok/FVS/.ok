# IDE Stuff
	  dev: $sln = ($null -eq $arg) ? ".\INPUT.sln" : ".\$arg.sln"; devenv $sln;  # Opens main solution by default or named sln
 dev-ffme: devenv .\Submodules\ffmediaelement\Unosquare.FFME.sln  # Opens FFME submodule

# Git commands
	fetch: git fetch --all; $(Get-Date) # Gets latest metadata and branches
	prune: git remote prune origin # Cleans up local branches that were deleted on DevOps

# Environment Stuff
	down-sdk: winget uninstall --id Microsoft.DotNet.SDK.6 # Removes the current .NET version 6 SDK
	up-sdk: winget install -e --id Microsoft.DotNet.SDK.6 -v 6.0.301 # Installs the version iNPUT-ACE is pinned to in global.json